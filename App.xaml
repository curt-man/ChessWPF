<Application x:Class="BoardGamesWPF.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:BoardGamesWPF"

             StartupUri="MVVM/View/MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="./Themes/TileCanvas.xaml"/>
                <ResourceDictionary Source="./Themes/CustomButtons.xaml"/>
                <ResourceDictionary Source="./Themes/BoardCoordinates.xaml"/>
                <ResourceDictionary Source="./Themes/CustomComboBox.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
        
    </Application.Resources>
</Application>
